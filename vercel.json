{
  "version": 2,
  "builds": [
    {
      "src": "my-app/next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "backend/marketplace_app/main.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "backend/marketplace_app/main.py"
    },
    {
      "source": "/(.*)",
      "destination": "my-app/main.py"
    }
  ],
  "functions": {
    "backend/marketplace_app/*.py": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "cleanUrls": true
}
