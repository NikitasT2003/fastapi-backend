{
  "version": 2,
  "builds": [
    {
      "src": "my-app/next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "backend/marketplace_app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/marketplace_app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "my-app/index.html"
    }
  ],
  "functions": {
    "backend/marketplace_app/*.py": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}
